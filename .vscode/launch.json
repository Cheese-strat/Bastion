{
    "version": "0.2.0",
    "configurations": [
        {
            "command": "npm run start:node",
            "name": "Execute with Node",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ],
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\dist\\src\\main.js",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}