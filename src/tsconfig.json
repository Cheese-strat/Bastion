{
    "compilerOptions": {
        "outDir": "./dist",
        "rootDir": "./src",
        "allowJs": true,
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    },
    "include": [
        "./src/**/**/*",
        "./src/commands/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}